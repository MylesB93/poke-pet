<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="PokePet.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:PokePet.Views"
    Shell.FlyoutBehavior="Flyout"
    Title="PokePet">

    <ShellContent
        Title="Start"
        ContentTemplate="{DataTemplate local:Start}"
        Route="Start" />
    
    <ShellContent
        Title="Search"
        ContentTemplate="{DataTemplate local:Search}"
        Route="Search" />

    <ShellContent
        Title="Your Pokemon"
        ContentTemplate="{DataTemplate local:PokemonListing}"
        Route="PokemonListing" />

</Shell>
